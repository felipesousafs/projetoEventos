<p id="notice"><%= notice %></p>

<h1>Inscriptions</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Paid</th>
      <th>Event</th>
      <th>User</th>
      <th>Coupom</th>
      <th>Paid at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inscriptions.each do |inscription| %>
      <tr>
        <td><%= inscription.paid %></td>
        <td><%= inscription.event.name %></td>
        <td><%= inscription.user.name %></td>
        <td><%= inscription.coupom %></td>
        <td><%= inscription.paid_at %></td>
        <td><%= link_to 'Show', inscription %></td>
        <td><%= link_to 'Destroy', inscription, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>