<li class="nested-fields event_item accordion" id="event_item">
  <div onclick="toggle_collapse(this);" style="width: 100%;">
    &nbsp;<a class='label label-warning pull-left abrir' id="abrir"> Mostrar/Ocultar  </a>
  </div>
  <div class='fundo-branco collapse' id="collapse_event_item">
    <div class="pull-right">
      <%= link_to_remove_association f do %>
        <i class='fa fa-trash-o btn btn-danger'> Remover</i>
      <% end %>
    </div>
    <div class="row">
      <div class="col-md-5">
        <%= f.input :name %>
      </div>
      <div class="col-md-5">
        <%= f.input :value %>
      </div>
    </div>
    <div class="row">
      <div class="col-md-5">
        <%= f.association :event_item_type %>
      </div>
      <div class="col-md-5">
        <%= f.input :description %>
      </div>
    </div>
  </div>
</li>